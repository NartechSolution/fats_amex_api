model AssetVerification {
    id                String   @id @default(uuid())
    tagNumber         String?
    assetCondition    String?
    assetStatus       String?
    brand             String?
    modal             String?
    serialNumber      String?
    faNumber          String?
    extNumber         String?
    locationId        String?
    assetCategoryId   String?
    assetOldTagNumber String?
    employeeId        String?
    assetDescription  String?
    image1            String?
    image2            String?
    image3            String?
    image4            String?
    image5            String?
    createdAt         DateTime @default(now())
    updatedAt         DateTime @updatedAt
}
